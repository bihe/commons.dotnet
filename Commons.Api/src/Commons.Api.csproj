<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>Binggl.Commons.Api</PackageId>
    <Version>1.0.2</Version>
    <Authors>Henrik Binggl</Authors>
    <Description>Logic, components used for my aspnet.core projects.</Description>
    <PackageLicenseUrl>https://raw.githubusercontent.com/bihe/commons.dotnet/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/bihe/commons.dotnet</PackageProjectUrl>
    <RepositoryUrl>https://github.com/bihe/commons.dotnet</RepositoryUrl>
    <IncludeSymbols>true</IncludeSymbols>
    <IncludeSource>true</IncludeSource>
  </PropertyGroup>

  <ItemGroup>

    <PackageReference Include="Microsoft.AspNetCore.Mvc" Version="2.1.1" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc.Razor " Version="2.1.1" />
    <PackageReference Include="Microsoft.Extensions.PlatformAbstractions" Version="1.1.0" />
  </ItemGroup>

</Project>
